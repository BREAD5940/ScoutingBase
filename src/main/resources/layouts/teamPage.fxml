      </menus>
    </MenuBar>
    <AnchorPane fx:id="basePane" maxHeight="-1.0" maxWidth="-1.0" prefHeight="726.0" prefWidth="1283.0" VBox.vgrow="ALWAYS">
         <children>
            <Button fx:id="backButton" graphicTextGap="0.0" layoutX="14.0" layoutY="14.0" onAction="#handleBackButton" pickOnBounds="true" prefHeight="26.0" prefWidth="54.0" style="-fx-background-color: #ffffff;" text="Back" textAlignment="CENTER">
               <font>
                  <Font name="System Bold" size="13.0" />
               </font>
               <effect>
                  <Lighting diffuseConstant="1.28" light="$null">
                     <bumpInput>
                        <Shadow blurType="ONE_PASS_BOX" />
                     </bumpInput>
                  </Lighting>
               </effect>
            </Button>
            <Label layoutX="542.0" layoutY="33.0" text="Team Name">
               <font>
                  <Font name="System Bold" size="36.0" />
               </font>
            </Label>
            <TextArea fx:id="teamText" editable="false" layoutX="18.0" layoutY="87.0" prefHeight="639.0" prefWidth="614.0" />
            <TextArea fx:id="pitText" editable="false" layoutX="643.0" layoutY="87.0" prefHeight="639.0" prefWidth="614.0" />
         </children>
    </AnchorPane>
