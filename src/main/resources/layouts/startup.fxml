<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.SeparatorMenuItem?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="409.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="base.controllers.StartupControl">
  <children>
    <MenuBar VBox.vgrow="NEVER">
      <menus>
        <Menu mnemonicParsing="false" text="File">
          <items>
            <MenuItem mnemonicParsing="false" text="New" />
            <MenuItem mnemonicParsing="false" text="Open…" />
            <Menu mnemonicParsing="false" text="Open Recent" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem mnemonicParsing="false" text="Close" />
            <MenuItem mnemonicParsing="false" text="Save" />
            <MenuItem mnemonicParsing="false" text="Save As…" />
            <MenuItem mnemonicParsing="false" text="Revert" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem mnemonicParsing="false" text="Preferences…" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem mnemonicParsing="false" text="Quit" />
          </items>
        </Menu>
        <Menu mnemonicParsing="false" text="Edit">
          <items>
            <MenuItem mnemonicParsing="false" text="Undo" />
            <MenuItem mnemonicParsing="false" text="Redo" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem mnemonicParsing="false" text="Cut" />
            <MenuItem mnemonicParsing="false" text="Copy" />
            <MenuItem mnemonicParsing="false" text="Paste" />
            <MenuItem mnemonicParsing="false" text="Delete" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem mnemonicParsing="false" text="Select All" />
            <MenuItem mnemonicParsing="false" text="Unselect All" />
          </items>
        </Menu>
        <Menu mnemonicParsing="false" text="Help">
          <items>
            <MenuItem mnemonicParsing="false" text="About MyHelloApp" />
          </items>
        </Menu>
      </menus>
    </MenuBar>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
            <Button layoutX="291.0" layoutY="243.0" mnemonicParsing="false" onAction="#handleGoButton" text="GO!" />
            <Label alignment="TOP_CENTER" layoutX="145.0" layoutY="66.0" prefHeight="50.0" prefWidth="351.0" text="Team 5940 ">
               <font>
                  <Font name="Trebuchet MS Bold" size="36.0" />
               </font>
            </Label>
            <Label alignment="TOP_CENTER" layoutX="193.0" layoutY="106.0" prefHeight="67.0" prefWidth="255.0" text="Scouting Base">
               <font>
                  <Font name="Futura Medium" size="36.0" />
               </font>
            </Label>
            <Label alignment="TOP_CENTER" layoutX="244.0" layoutY="186.0" prefHeight="29.0" prefWidth="136.0" text="Choose a session:">
               <font>
                  <Font name="Futura Medium" size="13.0" />
               </font>
            </Label>
            <ComboBox fx:id="sessionSelect" layoutX="237.0" layoutY="209.0" prefWidth="150.0" promptText="Session.." />
            <Button alignment="CENTER" layoutX="33.0" layoutY="295.0" mnemonicParsing="false" onAction="#handleBakeNew" prefHeight="57.0" prefWidth="59.0" text="Bake A New Session" textAlignment="CENTER" wrapText="true">
               <font>
                  <Font name="Futura Medium" size="10.0" />
               </font>
            </Button>
      </children>
    </AnchorPane>
  </children>
</VBox>
